<datastore-indexes autoGenerate="true">

    <datastore-index kind="Directory" ancestor="false" source="auto">
        <property name="parent" direction="asc"/>
        <property name="codeName" direction="asc"/>
    </datastore-index>

    <datastore-index kind="Task" ancestor="false" source="auto">
        <property name="parent" direction="asc"/>
        <property name="codeName" direction="asc"/>
    </datastore-index>

    <datastore-index kind="Image" ancestor="false" source="auto">
        <property name="parent" direction="asc"/>
        <property name="name" direction="asc"/>
    </datastore-index>

</datastore-indexes>
