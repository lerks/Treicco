<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:DialogBox animationEnabled="true" glassEnabled="true" autoHide="false" modal="true">
		<g:caption>
			<b>Add Directory</b>
		</g:caption>
		<g:FlowPanel>
			<g:Label>Code Name</g:Label>
			<g:TextBox ui:field="codename"></g:TextBox>
			<g:Label>Full Name</g:Label>
			<g:TextBox ui:field="fullname"></g:TextBox>
			<g:Button ui:field="okButton">Okay</g:Button>
			<g:Button ui:field="cancelButton">Cancel</g:Button>
		</g:FlowPanel>
	</g:DialogBox>

</ui:UiBinder> 